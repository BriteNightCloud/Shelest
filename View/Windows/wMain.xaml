<Window x:Class="Shelest.View.Windows.wMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shelest.View.Windows"
        mc:Ignorable="d"
        Title="{Binding ElementName=frmMain,Path=Content.Title}" Height="500" Width="600" MinHeight="500" MinWidth="600">
    <Grid>
        <Frame x:Name="frmMain" Source="../Pages/pMain.xaml" NavigationUIVisibility="Hidden" Focusable="False" IsTabStop="False"/>
    </Grid>
</Window>
